package ch15;

public class Taxi {

	int taxiNumber;
	String taxiName;
	int passengerCount;
	int money;
	
	public Taxi(int taxiNumber, String taxiName) {
		this.taxiNumber = taxiNumber;
		this.taxiName = taxiName;
	}
	
	public void take(int money) {
		this.money += money;
		passengerCount++;
	}
	
	public void showTaxiInfo() {
		
	}
}
